t = int(input())
for _ in range(t):
    X,Y=0,0
    flag = False
    n=int(input())
    s = input()
    for i in s:
        if i=='L':
            X = X-1 
        elif i=='R':
            X = X+1
        elif i=='U':
            Y = Y+1 
        else:
            Y=Y-1
        if X==1 and Y==1:
            flag = True
            break
    if flag:
        print("YES")
    else:
        print("NO")
