t = int(input())
for _ in range(t):
   n = int(input())
   al = ['a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', 'i', 'j', 'k', 'l', 'm', 'n', 'o', 'p', 'q', 'r', 's', 't', 'u', 'v', 'w', 'x', 'y', 'z']
   if n <= 28:
      _1 = 1
      _2 = 1
      _3 = n-2
   elif n <= 53:
      _1 = 1
      _2 = n - 26 - 1
      _3 = 26
   else:
      _1 = n - 26 - 26
      _2 = 26
      _3 = 26
   print(al[_1-1]+al[_2-1]+al[_3-1])
